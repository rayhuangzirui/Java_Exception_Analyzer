{
  "name": "vscode-extension",
  "displayName": "Java Exception Analyzer",
  "version": "1.0.0",
  "description": "",
  "main": "out/Extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "tsc -p ./ && vsce package"
  },
  "engines": {
    "vscode": "^1.98.0"
  },
  "activationEvents": [
    "onLanguage:java",
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-extension.sayHello",
        "title": "Hello World"
      },
      {
        "command": "vscode-extension.analyzeJava",
        "title": "Analyze Java"
      },
      {
        "command": "vscode-extension.configJavaAnalyzer",
        "title": "Configure Java Analyzer"
      },
      {
        "command": "vscode-extension.configReset",
        "title": "Reset Config for Java Analyzer"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "publisher": "rayhuangzirui",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.13.13",
    "@types/vscode": "^1.98.0",
    "typescript": "^5.8.2"
  }
}
